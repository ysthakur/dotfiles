{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palettes": {
    "list": {
      "dark": {
        "foreground": "#ffffff"
      },
      "light": {
        "foreground": "#000000"
      }
    },
    "template": "{{ if eq .Env.LIGHT_THEME \"true\" }}light{{ else }}dark{{ end }}"
  },
  "transient_prompt": {
    "template": "{{if .Root}}âš¡ {{end}}\uf054 ",
    "foreground": "#FFD700",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#ffff66",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#d2ff5e",
          "powerline_symbol": "\ue0b0",
          "background": "#003543",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mixed_threshold": "5",
            "style": "mixed"
          },
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "background": "#FFFB38",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf308 {{ .Context }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#0B59E7",
          "type": "docker",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "template": " \ue235 {{ .Venv }} ",
          "foreground": "#003544",
          "powerline_symbol": "\ue0b0",
          "background": "#0087D8",
          "type": "python",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf308 {{ .Context }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b2",
          "background": "#0B59E7",
          "type": "docker",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b2",
          "background": "#d2ff5e",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#242424",
          "powerline_symbol": "\ue0b2",
          "background": "#33DD2D",
          "type": "status",
          "style": "powerline",
          "background_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "threshold": 300
          },
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "p:foreground",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf054 ",
          "foreground": "p:foreground",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
